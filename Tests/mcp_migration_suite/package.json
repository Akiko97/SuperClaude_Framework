{
  "name": "@superclaude/mcp-migration-test-suite",
  "version": "1.0.0",
  "description": "Comprehensive testing framework for SuperClaude MCP migration",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:performance": "jest --testPathPattern=performance",
    "test:migration": "jest --testPathPattern=migration",
    "test:load": "jest --testPathPattern=load --runInBand",
    "test:stress": "jest --testPathPattern=stress --runInBand",
    "test:debug": "jest --verbose --no-cache",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist coverage test-results .nyc_output",
    "start:mcp-servers": "node scripts/start-mcp-servers.js",
    "stop:mcp-servers": "node scripts/stop-mcp-servers.js",
    "setup:test-env": "node scripts/setup-test-environment.js",
    "teardown:test-env": "node scripts/teardown-test-environment.js",
    "benchmark": "node scripts/run-benchmarks.js",
    "monitor": "node scripts/monitor-performance.js"
  },
  "dependencies": {
    "@types/node": "^20.8.0",
    "axios": "^1.5.0",
    "ws": "^8.14.0",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "cli-table3": "^0.6.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/uuid": "^9.0.4",
    "@types/lodash": "^4.14.199",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-html-reporters": "^3.1.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "testcontainers": "^10.2.1"
  },
  "keywords": [
    "superclaude",
    "mcp",
    "testing",
    "migration",
    "claude-code",
    "integration-testing",
    "performance-testing"
  ],
  "author": "SuperClaude Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}