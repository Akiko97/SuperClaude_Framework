{
  "name": "@superclaude/test-framework",
  "version": "1.0.0",
  "description": "Comprehensive testing suite for SuperClaude MCP servers Phase 3",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:unit": "jest --testMatch='**/*.unit.test.ts'",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:e2e": "jest --testMatch='**/*.e2e.test.ts'",
    "test:performance": "jest --testMatch='**/*.performance.test.ts'",
    "test:coverage": "jest --coverage",
    "test:regression": "jest --testMatch='**/*.regression.test.ts'",
    "test:migration": "jest --testMatch='**/*.migration.test.ts'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance",
    "test:phase3": "jest --testMatch='**/phase3/**/*.test.ts'",
    "benchmark": "node dist/benchmarks/runner.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@superclaude/shared": "workspace:*",
    "@superclaude/quality-server": "workspace:*",
    "@superclaude/performance-server": "workspace:*",
    "@superclaude/tasks-server": "workspace:*",
    "@superclaude/orchestrator-server": "workspace:*",
    "@superclaude/code-server": "workspace:*",
    "@superclaude/intelligence-server": "workspace:*",
    "@superclaude/ui-server": "workspace:*",
    "@superclaude/bridge-hooks": "workspace:*",
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "node-fetch": "^3.3.2",
    "date-fns": "^2.30.0",
    "benchmark": "^2.1.4",
    "p-limit": "^4.0.0",
    "nanoid": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.8",
    "@types/benchmark": "^2.1.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "keywords": [
    "superclaude",
    "mcp",
    "testing",
    "phase3",
    "quality",
    "performance",
    "integration"
  ],
  "author": "SuperClaude Team",
  "license": "MIT"
}