{
  "name": "superclaude-builder",
  "version": "1.0.0",
  "description": "SuperClaude Semantic Code Generation Engine - Advanced code generation and modification with semantic understanding",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "code-generation",
    "semantic-editing",
    "refactoring",
    "typescript",
    "framework-integration"
  ],
  "author": "SuperClaude MCP",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "typescript": "^5.0.0",
    "vscode-languageserver-protocol": "^3.17.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "jscodeshift": "^0.15.0",
    "prettier": "^3.0.0",
    "eslint": "^8.45.0",
    "zod": "^3.22.0",
    "winston": "^3.10.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jscodeshift": "^0.11.6",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ]
  }
}